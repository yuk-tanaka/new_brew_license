{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "fabpot/goutte": "^3.2",
    "hinaloe/laravel-line-notify": "^0.0.1",
    "illuminate/notifications": "^5.8",
    "laravel/lumen-framework": "5.8.*",
    "league/flysystem": "^1.0",
    "ramsey/uuid": "^3.8",
    "vlucas/phpdotenv": "^3.3",
    "vluzrmos/tinker": "^1.4"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.26.1",
    "doctrine/dbal": "^2.9",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
